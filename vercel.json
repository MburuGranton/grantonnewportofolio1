{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "framework": "vite",
  "functions": {
    "api/contact.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/contact",
      "dest": "/api/contact.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}